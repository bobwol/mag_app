<%= form_for @push, :html => { :class => 'form-horizontal form-model' } do |f| %>
  <div class="form-group">
    <%= f.label :push_message, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :push_message, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :delivery_time, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.datetime_select :delivery_time, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :push_app_id, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :push_app_id, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :push_client_key, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :push_client_key, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :push_restapi_key, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :push_restapi_key, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :appstore_prod_cert, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :appstore_prod_cert, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :appstore_dev_cert, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :appstore_dev_cert, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :gcm_sender_id, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :gcm_sender_id, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :gcm_api_key, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :gcm_api_key, :class => 'text-field form-control' %>
    </div>
  </div>

  <div class="form-group">
    <div class='col-md-offset-2 col-md-10'>
      <%= f.submit nil, :class => 'btn btn-primary' %>
      <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                  pushes_path, :class => 'btn btn-default' %>
    </div>
  </div>
<% end %>
