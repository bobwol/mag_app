<h1>Listing statistics</h1>

<table>
  <thead>
    <tr>
      <th>Ios installs</th>
      <th>Android installs</th>
      <th>App</th>
      <th>Users total</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @statistics.each do |statistic| %>
      <tr>
        <td><%= statistic.ios_installs %></td>
        <td><%= statistic.android_installs %></td>
        <td><%= statistic.app %></td>
        <td><%= statistic.users_total %></td>
        <td><%= statistic.user %></td>
        <td><%= link_to 'Show', statistic %></td>
        <td><%= link_to 'Edit', edit_statistic_path(statistic) %></td>
        <td><%= link_to 'Destroy', statistic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Statistic', new_statistic_path %>
